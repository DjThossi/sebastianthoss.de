<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!--> <html lang="en"> <!--<![endif]-->
<head>
    <title>{{ page.title|raw }} - Sebastian Thoss</title>

    <!-- Meta -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    {% if page.meta.description %}
        <meta name="description" content="{{ page.meta.description|raw }}" />
    {% elseif page.overview.intro %}
        <meta name="description" content="{{ page.overview.intro|raw }}" />
    {% else %}
        <meta name="description" content="{{ metaDescription|raw }}" />
    {% endif %}

    <meta name="keywords" content="Sebastian, Thoss, {% if page.meta.keywords %}{{ page.meta.keywords|join(', ')|raw }}, {% endif %}{% if metaKeywords %}{{ metaKeywords|join(', ')|raw }}, {% endif %}Tho&szlig;, Workshops, PHP, Hamburg, Ahrensburg, Dublin, Sydney, Rostock, Plauen, Putbus" />
    <meta name="author" content="Sebastian Thoss" />

    {% if page.sitemap is defined and page.sitemap == false %}
        <meta name=”robots” content=”noindex,nofollow” />
    {% else %}
        <meta name=”robots” content=”index,follow” />
    {% endif %}

    {% if page.meta.image %}
        <meta name="og:image" content="{{ site.url }}{{ page.meta.image|raw }}" />
    {% elseif page.overview.image %}
        <meta name="og:image" content="{{ site.url }}{{ page.overview.image.url|raw }}" />
    {% endif %}

    {% if page.hreflang %}
        <link rel="alternate" hreflang="de" href="{{ site.url }}{{ page.hreflang.de.href }}" />
        <link rel="alternate" hreflang="en" href="{{ site.url }}{{ page.hreflang.en.href }}" />
    {% endif %}

    <!-- Favicon -->
    <link rel="shortcut icon" href="{{ site.url }}/assets/img/sebastian_thoss.jpg" />

    <!-- Web Fonts -->
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:400,300,600&subset=cyrillic,latin" />

    <!-- CSS Global Compulsory -->
    <link rel="stylesheet" href="{{ site.url }}/assets/plugins/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="{{ site.url }}/assets/css/style.css" />

    <!-- CSS Header and Footer -->
    <link rel="stylesheet" href="{{ site.url }}/assets/css/headers/header-v6.css" />
    <link rel="stylesheet" href="{{ site.url }}/assets/css/footers/footer-v2.css" />

    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="{{ site.url }}/assets/plugins/animate.css" />
    <link rel="stylesheet" href="{{ site.url }}/assets/plugins/line-icons/line-icons.css" />
    <link rel="stylesheet" href="{{ site.url }}/assets/plugins/font-awesome/css/font-awesome.min.css" />

    <!-- CSS Page Style -->
    {% for css_entry in layoutAddCss %}
        <link rel="stylesheet" href="{{ site.url }}{{ css_entry }}" />
    {% endfor %}
    {% for css_entry in page.add_css %}
        <link rel="stylesheet" href="{{  site.url }}{{ css_entry }}" />
    {% endfor %}

    <!--[if lt IE 9]>
    {% for css_entry in page.add_css_lt_ie_9 %}
        <link rel="stylesheet" href="{{ site.url }}{{ css_entry }}" />
    {% endfor %}
    <![endif]-->

    <!-- CSS Theme -->
    <link rel="stylesheet" href="{{ site.url }}/assets/css/theme-colors/blue.css" id="style_color" />
    <link rel="stylesheet" href="{{ site.url }}/assets/css/theme-skins/dark.css" />

    <!-- CSS Customization -->
    <link rel="stylesheet" href="{{ site.url }}/assets/css/custom.css" />
</head>

<body class="header-fixed header-fixed-space">
<div class="wrapper">
{% block layout %}{% endblock %}
</div><!--/wrapper-->

<!-- JS Global Compulsory -->
<script type="text/javascript" src="{{ site.url }}/assets/plugins/jquery/jquery.min.js"></script>
<script type="text/javascript" src="{{ site.url }}/assets/plugins/jquery/jquery-migrate.min.js"></script>
<script type="text/javascript" src="{{ site.url }}/assets/plugins/bootstrap/js/bootstrap.min.js"></script>

<!-- JS Implementing Plugins -->
<script type="text/javascript" src="{{ site.url }}/assets/plugins/back-to-top.js"></script>
<script type="text/javascript" src="{{ site.url }}/assets/plugins/jquery.parallax.js"></script>

<!-- JS Customization -->
<script type="text/javascript" src="{{ site.url }}/assets/js/custom.js"></script>

<!-- JS Page Level -->
<script type="text/javascript" src="{{ site.url }}/assets/js/app.js"></script>
{% for js_script in page.js.scripts %}
    {% if js_script starts with 'http' or js_script starts with '//' %}
        <script type="text/javascript" src="{{ js_script }}"></script>
    {% else %}
        <script type="text/javascript" src="{{ site.url }}{{ js_script }}"></script>
    {% endif %}
{% endfor %}

<script type="text/javascript">
    jQuery(document).ready(function() {
        App.init();
        {% for codeline in page.js.run_code %}
            {{ codeline }}
        {% endfor %}
    });
</script>

<!--[if lt IE 10]>
{% for js_script in page.js.scripts_lt_ie_10 %}
    <script type="text/javascript" src="{{ site.url }}{{ js_script }}"></script>
{% endfor %}

<![endif]-->

<!--[if lt IE 9]>
<script src="{{  site.url }}/assets/plugins/respond.js"></script>
<script src="{{  site.url }}/assets/plugins/html5shiv.js"></script>
<script src="{{  site.url }}/assets/plugins/placeholder-IE-fixes.js"></script>
{% for js_script in page.js.scripts_lt_ie_9 %}
 <script type="text/javascript" src="{{  site.url }}{{ js_script }}"></script>
{% endfor %}
<![endif]-->

<!-- Begin Cookie Consent plugin by Silktide - https://silktide.com/cookieconsent -->
<script type="text/javascript">
    window.cookieconsent_options = {
        "message":"{{ cookieConsentMessage }}",
        "dismiss":"{{ cookieConsentDismiss }}",
        "learnMore":"{{ cookieConsentLearnMore }}",
        "link":"{{ cookieConsentLink }}",
        "theme":"light-bottom"
    };
</script>

<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/1.0.10/cookieconsent.min.js"></script>
<!-- End Cookie Consent plugin -->

<!-- Global Site Tag (gtag.js) - Google Analytics -->
<script>
    var gaProperty = 'UA-3423622-11';
    var disableStr = 'ga-disable-' + gaProperty;

    if (document.cookie.indexOf(disableStr + '=true') > -1) {
        window[disableStr] = true;
    }

    function gaOptout() {
        document.cookie = disableStr + '=true; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/';
        window[disableStr] = true;
    }
</script>

<script async src="https://www.googletagmanager.com/gtag/js?id=UA-3423622-11"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){
        dataLayer.push(arguments)
    }

    gtag('js', new Date());

    gtag('config', 'UA-3423622-11', {'anonymize_ip': true});

    function gaHeartbeat(){
        gtag('event', 'heartbeat');
        setTimeout(gaHeartbeat, 15*1000);
    }
    gaHeartbeat();
</script>
<!-- Google Analytics -->
</body>
</html>
